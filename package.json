{
  "name": "azure-function-orm",
  "version": "1.0.0",
  "description": "",
  "license": "MIT",
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "clean": "rimraf dist",
    "prestart": "npm run clean && npm run build",
    "start": "func start",
    "test": "echo \"TODO: No tests yet...\"",
    "lint": "eslint .",
    "mig:create": "npx typeorm-ts-node-commonjs migration:create src/migrations/$npm_config_name",
    "mig:generate": "npx typeorm-ts-node-commonjs migration:generate src/migrations/$npm_config_name -d src/db/dataSource.ts",
    "mig:run": "npx typeorm-ts-node-commonjs migration:run -d src/db/dataSource.ts",
    "mig:revert": "npx typeorm-ts-node-commonjs migration:revert -d src/db/dataSource.ts"
  },
  "dependencies": {
    "@automapper/classes": "^8.8.1",
    "@automapper/core": "^8.8.1",
    "@azure/functions": "^4.0.0",
    "dotenv": "^16.4.1",
    "mssql": "^9.1.1",
    "reflect-metadata": "^0.1.14",
    "ts-node": "^10.9.2",
    "typeorm": "^0.3.20",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/node": "^18.x",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-simple-import-sort": "^12.0.0",
    "prettier": "3.2.5",
    "rimraf": "^5.0.0",
    "typescript": "^4.0.0"
  },
  "main": "dist/src/index.js"
}